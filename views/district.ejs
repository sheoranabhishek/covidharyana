        <script type=text/javascript>


var datadx = null;

var getdet = new XMLHttpRequest();
getdet.withCredentials = false;

getdet.addEventListener("readystatechange", function () {
  if (this.readyState === this.DONE) {
    var statex = JSON.parse(this.responseText);
    statex = statex.Haryana.districtData;
    console.log(statex);
    var dis =  Object.keys(statex);
    console.log(dis);
    var i=0; 
    var j=0;
    var m=0;
    var datx="";
    // var statename = Object.keys(statex);
    		     for(i in statex){

                  datx+= `
                  <tr>
                    <th scope="row" class="font-weight-bold text-dark" style="color:black;">
                    `+
                    dis[j]
                    +`
                    </th>
                    <td class="font-weight-bold bg-blue text-white">`+
                    statex[i].confirmed

                    +`</td>
                    <td class="font-weight-bold bg-success text-white">`+
                    statex[i].recovered
                    +`</td>
                    <td class="font-weight-bold bg-danger text-white">`+
                    statex[i].deceased
                    +`</td>
                    
                  </tr>`;
                  j++;
               }



              document.getElementById("districtx").innerHTML=`
              <table class="table align-items-center table-flush">
                <thead class="thead-light">
                  <tr>
                    <th scope="col">District Name</th>
                    <th scope="col">Total Cases</th>
                    <th scope="col">Total Recovered</th>
                    <th scope="col">Total Death</th>



                  </tr>
                  </thead>
                  <tbody>
                  ` 
                  + datx +
                `


                </tbody>
              </table>

               `;
  }
});

getdet.open("GET", "https://api.covid19india.org/state_district_wise.json");

getdet.send(datadx);


        </script>

